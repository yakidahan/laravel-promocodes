{
    "name": "yadahan/promocodes",
    "description": "Promotional Codes Generator for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaakov Dahan",
            "email": "yakidehan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^2.10|^3.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "~4.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Gabievi\\Promocodes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gabievi\\Promocodes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Gabievi\\Promocodes\\PromocodesServiceProvider"
            ],
            "aliases": {
                "Promocodes": "Gabievi\\Promocodes\\Facades\\Promocodes"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
