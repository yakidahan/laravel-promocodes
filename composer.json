{
  "name": "yakidahan/promocodes",
  "description": "Promotional Codes Generator for Laravel",
  "keywords": [
    "promo code",
    "coupon code",
    "discount code",
    "laravel",
    "php"
  ],
  "homepage": "https://github.com/yakidahan/promocodes",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Yaakov Dahan",
      "email": "yakidehan@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "doctrine/dbal": "^2.10|^3.0",
    "laravel/framework": "^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "~4.0",
    "phpunit/phpunit": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "Gabievi\\Promocodes\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gabievi\\Promocodes\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gabievi\\Promocodes\\PromocodesServiceProvider"
      ],
      "aliases": {
        "Promocodes": "Gabievi\\Promocodes\\Facades\\Promocodes"
      }
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
